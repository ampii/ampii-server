<?xml version="1.0" encoding="UTF-8"?>
<CSML xmlns="http://bacnet.org/CSML/1.3">
    <Definitions>
    <Sequence name="WXXM-WX.AbstractMeasure" documentation="Abstract measurement class. Each measurment has a unit of measure, uom. WXXM allows more than one uom -- e.g., F/C/K for temperature. FSGIM only allows one choice of uom for a physical quantity.">
        <Real name="value" documentation="The value of the measure."/>
        <!-- associations -->
        <Enumerated name="powerOfTenMultiplier" type="Fsgim.SiScaleCodeType" optional="true" documentation="Default is 1."/>
        <Enumerated name="uom" type="Fsgim.UnitSymbolKind" documentation="Units of measure for the measurment."/>
    </Sequence>

    <Sequence name="WXXM-WX.Angle" extends="WXXM-WX.AbstractMeasure" documentation="Generic angle class.">
        <!-- associations -->
        <Enumerated name="uom" type="Fsgim.UnitSymbolKind" documentation="
            Unit of measure for angle.
            (This association redefines the uom association between the AbstractMeasure Class (see Clause 5.7.6.6.2.1.1) and the UnitSymbolKind Enumeration (see Clause 6.6.2.6).)
            Constraint:  must be rad  
            "/>
    </Sequence>

    <Sequence name="WXXM-WX.Bearing" documentation="Bearing is used to represent direction in the 2D coordinate reference system. It is an &quot;angle measured from true north&quot;. It is represented as an angle restricted to the range greater than 0 and up to and including 360 degrees which is true north.">
        <Integer name="maxInclusive" documentation="The value of true north, 360."/>
        <Integer name="minInclusive" documentation="Must be greater than 0."/>
    </Sequence>

    <Sequence name="WXXM-WX.Distance" extends="WXXM-WX.AbstractMeasure" documentation="Generic distance class.">
        <!-- associations -->
        <Enumerated name="uom" type="Fsgim.UnitSymbolKind" documentation="
            Units of measure for distance.
            (This association redefines the uom association between the AbstractMeasure Class (see Clause 5.7.6.6.2.1.1) and the UnitSymbolKind Enumeration (see Clause 6.6.2.6).)
            Constraint:  must be m  
            "/>
    </Sequence>

    <Sequence name="WXXM-WX.Luminance" extends="WXXM-WX.AbstractMeasure" documentation="Density of luminous intenstity.">
        <!-- associations -->
        <Enumerated name="uom" type="Fsgim.UnitSymbolKind" documentation="
            Unit of measure for luminance.
            (This association redefines the uom association between the AbstractMeasure Class (see Clause 5.7.6.6.2.1.1) and the UnitSymbolKind Enumeration (see Clause 6.6.2.6).)
            Constraint:  must be sb  
            "/>
    </Sequence>

    <Sequence name="WXXM-WX.Percentage" documentation="Generic percentage class; value is limited to between 0 and 100 inclusive.">
        <Integer name="maxInclusive" documentation="Maximum inclusive value."/>
        <Integer name="minInclusive" documentation="Minimum inclusive value."/>
    </Sequence>

    <Sequence name="WXXM-WX.Pressure" extends="WXXM-WX.AbstractMeasure" documentation="Generic pressure class.">
