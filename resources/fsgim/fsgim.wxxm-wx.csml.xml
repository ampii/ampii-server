        <Boolean name="patchy" optional="true" documentation="True if the hydrometeor is patchy."/>
        <!-- associations -->
        <Enumerated name="type" type="WXXM-WX.HydrometeorTypes" optional="true" documentation="Hydrometeor type."/>
