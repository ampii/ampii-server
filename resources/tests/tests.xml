<?xml version="1.0" encoding="UTF-8"?>
<CSML xmlns="http://bacnet.org/csml/1.2"  defaultLocale="en-US" >

    <!-- parameters for the test environment used by Test.java of java classes containing tests -->

    <Composition name="test-pars" type="org.ampii.types.TestEnvironment">
        <Unsigned  name="covCallbackFailTime"        value="1100" units="msec" />
        <Unsigned  name="logCallbackFailTime"        value="1100" units="msec" />
        <Unsigned  name="subsRecordRemovalFailTime"  value="1100" units="msec" />
        <Unsigned  name="multiRecordRemovalFailTime" value="1100" units="msec" />
    </Composition>

    <!-- list of java classes containing tests to run -->

    <List name="test-list">
        <!--<String value="org.ampii.xd.test.tests.ClientTests"/>-->
