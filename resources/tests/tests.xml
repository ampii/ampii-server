        <String value="org.ampii.xd.test.tests.TypeTests"/>
        <String value="org.ampii.xd.test.tests.IncludeTests"/>
        <String value="org.ampii.xd.test.tests.AuthTests"/>
        <String value="org.ampii.xd.test.tests.RangeTests"/>
        <String value="org.ampii.xd.test.tests.SkipTests"/>
        <String value="org.ampii.xd.test.tests.DefinitionsTests"/>
        <String value="org.ampii.xd.test.tests.LinkTests"/>
        <String value="org.ampii.xd.test.tests.LocalizationTests"/>
        <String value="org.ampii.xd.test.tests.MultiTests"/>
