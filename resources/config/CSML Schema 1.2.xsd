<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns="http://bacnet.org/CSML/1.3"
           targetNamespace="http://bacnet.org/CSML/1.3"
           elementFormDefault="qualified" >

    <!-- THIS IS NOT AN OFFICIAL SCHEMA. It may be helpful for HUMAN development but it slightly broken against the current spec.  -->

    <xs:element name="CSML" type="CSML"/>
    <xs:complexType name="CSML">
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="Definitions"/>
                <xs:element ref="Includes"/>
                <xs:group ref="AllDataGroup"/>
                <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
            </xs:choice>
        </xs:sequence>
        <xs:attribute name="defaultLocale" type="xs:language" use="optional"/>
        <xs:anyAttribute namespace="##other" processContents="lax"/>
    </xs:complexType>


    <xs:complexType name="DataBaseType">
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded"> <!-- repeatable elements -->
                <xs:element ref="Definitions"/>
                <xs:element name="DisplayName" type="LocalizableStringType"/>
                <xs:element name="Description" type="LocalizableStringType"/>
                <xs:element name="Documentation">
                    <xs:complexType mixed="true">
                        <xs:sequence>
                            <xs:any namespace="##any" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
                        </xs:sequence>
                        <xs:attribute name="locale" type="xs:language" use="optional"/>
                        <xs:anyAttribute namespace="##any" processContents="lax"/>
                    </xs:complexType>
                </xs:element>
                <xs:element name="WritableWhen" type="LocalizableStringType"/>
                <xs:element name="RequiredWhen" type="LocalizableStringType"/>
                <xs:element name="Error" type="LocalizableStringType"/>
                <xs:element ref="Links"/>
                <xs:element ref="Failures"/>
                <xs:element ref="ValueTags"/>
                <xs:element ref="Revisions"/>
                <xs:element ref="History"/>
                <xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
            </xs:choice>
            <xs:element name="PriorityArray" type="ArrayType" minOccurs="0"/>
            <xs:element ref="Extensions" minOccurs="0"/>
        </xs:sequence>
        <!-- these are in the same order as in the standard -->
        <xs:attribute name="name" type="xs:string" use="optional"/>
        <xs:attribute name="id" type="xs:string" use="optional"/>
        <xs:attribute name="type" type="xs:string" use="optional"/>
        <xs:attribute name="extends" type="xs:string" use="optional"/>
        <xs:attribute name="overlays" type="xs:string" use="optional"/>
        <xs:attribute name="nodeType" type="xs:string" use="optional"/>    <!-- TODO: make NodeTypeType -->
        <xs:attribute name="nodeSubType" type="xs:string" use="optional"/>
        <xs:attribute name="displayName" type="xs:string" use="optional"/>
        <xs:attribute name="description" type="xs:string" use="optional"/>
        <!-- <Documentation> defined above, 'documentation' attribute defined below -->
        <xs:attribute name="documentation" type="xs:string" use="optional"/>
        <xs:attribute name="comment" type="xs:string" use="optional"/>
        <xs:attribute name="writable" type="xs:boolean" use="optional"/>
        <xs:attribute name="commandable" type="xs:boolean" use="optional"/>
        <!-- <PriorityArray> is defined above -->
        <xs:attribute name="relinquishDefault" type="xs:anyType" use="optional"/>
        <!-- <Failures> is defined above -->
        <xs:attribute name="readable" type="xs:boolean" use="optional"/>
        <xs:attribute name="associatedWith" type="xs:string" use="optional"/>
        <xs:attribute name="requiredWith" type="xs:string" use="optional"/>
        <xs:attribute name="requiredWithout" type="xs:string" use="optional"/>
        <xs:attribute name="notPresentWith" type="xs:string" use="optional"/>
        <xs:attribute name="writeEffective" type="WriteEffectiveType" use="optional"/>
        <xs:attribute name="optional" type="xs:boolean" use="optional"/>
        <xs:attribute name="absent" type="xs:boolean" use="optional"/>
        <xs:attribute name="variability" type="VariabilityType" use="optional"/>
        <xs:attribute name="volatility" type="VolatilityType" use="optional"/>
        <xs:attribute name="isMultiLine" type="xs:boolean" use="optional"/>
        <xs:attribute name="inAlarm" type="xs:boolean" use="optional"/>
        <xs:attribute name="overridden" type="xs:boolean" use="optional"/>
        <xs:attribute name="fault" type="xs:boolean" use="optional"/>
        <xs:attribute name="outOfService" type="xs:boolean" use="optional"/>
        <!-- <Links> is defined above -->
        <xs:attribute name="tags" type="xs:string" use="optional"/>
        <!-- <ValueTags> is defined above -->
        <xs:attribute name="authRead" type="xs:string" use="optional"/>
        <xs:attribute name="authWrite" type="xs:string" use="optional"/>
        <xs:attribute name="authVisible" type="xs:string" use="optional"/>
        <xs:attribute name="href" type="xs:anyURI" use="optional"/>
        <xs:attribute name="sourceId" type="xs:string" use="optional"/>
        <xs:attribute name="etag" type="xs:string" use="optional"/>
        <!-- 'count' has no representation in XML -->
        <!-- 'children' has no representation in XML -->
        <!-- 'descendants' has no representation in XML -->
        <!-- <History> is defined above -->
        <xs:attribute name="targetType" type="xs:string" use="optional"/>
        <xs:attribute name="relationship" type="xs:string" use="optional"/>
        <!-- <Error> defined above, 'documentation' attribute defined below -->
        <xs:attribute name="error" type="xs:nonNegativeInteger" use="optional"/>
        <xs:attribute name="displayOrder" type="xs:nonNegativeInteger" use="optional"/>
        <xs:attribute name="published" type="xs:dateTime" use="optional"/>
        <xs:attribute name="updated" type="xs:dateTime" use="optional"/>
        <xs:attribute name="writableWhen" type="WritableWhenType" use="optional"/>
        <xs:attribute name="requiredWhen" type="RequiredWhenType" use="optional"/>
        <xs:attribute name="contextTag" type="xs:nonNegativeInteger" use="optional"/>
        <xs:attribute name="propertyIdentifier" type="xs:nonNegativeInteger" use="optional"/>
        <xs:attribute name="addRev" type="xs:string" use="optional"/>
        <xs:attribute name="remRev" type="xs:string" use="optional"/>
        <xs:attribute name="modRev" type="xs:string" use="optional"/>
        <xs:attribute name="dataRev" type="xs:string" use="optional"/>
        <!-- <Revisions> defined above -->
        <xs:anyAttribute namespace="##other" processContents="lax"/>
    </xs:complexType>


    <xs:complexType name="PrimitiveDataBaseType">
        <xs:complexContent>
            <xs:extension base="DataBaseType">
                <xs:attribute name="displayNameForWriting" type="xs:string" use="optional"/>
                <xs:attribute name="notForWriting" type="xs:boolean" use="optional"/>
                <xs:attribute name="notForReading" type="xs:boolean" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ConstructedDataBaseType">
        <xs:complexContent>
            <xs:extension base="DataBaseType">
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="AllDataGroup"/>
                </xs:choice>
                <xs:attribute name="truncated" type="xs:boolean" use="optional"/>
                <xs:attribute name="partial" type="xs:boolean" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="CollectionsDataBaseType">
        <xs:complexContent>
            <xs:extension base="DataBaseType">
                <xs:sequence>
                    <xs:element ref="MemberTypeDefinition" minOccurs="0" maxOccurs="1"/>
                    <xs:choice minOccurs="0" maxOccurs="unbounded">
                        <xs:group ref="AllDataGroup"/>
                    </xs:choice>
                </xs:sequence>
                <xs:attribute name="memberType" type="xs:token" use="optional"/>
                <xs:attribute name="truncated" type="xs:boolean" use="optional"/>
                <xs:attribute name="partial" type="xs:boolean" use="optional"/>
                <xs:attribute name="maximumSize" type="xs:nonNegativeInteger" use="optional"/>
                <xs:attribute name="minimumSize" type="xs:nonNegativeInteger" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Any" type="AnyType"/>
    <xs:complexType name="AnyType">
        <xs:complexContent>
            <xs:extension base="DataBaseType">
                <xs:attribute name="allowedTypes" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Links" type="LinksType"/>
    <xs:complexType name="LinksType">
        <xs:sequence>
            <xs:element ref="Link" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:anyAttribute namespace="##other" processContents="lax"/>
    </xs:complexType>

    <xs:element name="Failures" type="FailuresType"/>
    <xs:complexType name="FailuresType">
        <xs:sequence>
            <xs:element ref="Link" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:anyAttribute namespace="##other" processContents="lax"/>
    </xs:complexType>

    <xs:element name="ValueTags" type="ValueTagsType"/>
    <xs:complexType name="ValueTagsType">
        <xs:complexContent>
            <xs:extension base="CollectionsDataBaseType"/>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Revisions" type="RevisionsType"/>
    <xs:complexType name="RevisionsType">
        <xs:complexContent>
            <xs:extension base="CollectionsDataBaseType"/>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="History" type="HistoryType"/>
    <xs:complexType name="HistoryType">
        <xs:sequence>
            <xs:element ref="Sequence" minOccurs="0" maxOccurs="unbounded"/> <!-- TODO fill out the type of these <Sequence>s -->
        </xs:sequence>
        <xs:anyAttribute namespace="##other" processContents="lax"/>
    </xs:complexType>


    <xs:element name="Collection" type="CollectionType"/>
    <xs:complexType name="CollectionType">
        <xs:complexContent>
            <xs:extension base="CollectionsDataBaseType"/>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Array" type="ArrayType"/>
    <xs:complexType name="ArrayType">
        <xs:complexContent>
            <xs:extension base="CollectionsDataBaseType"/>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Unknown" type="UnknownType"/>
    <xs:complexType name="UnknownType">
        <xs:complexContent>
            <xs:extension base="CollectionsDataBaseType"/>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Bit" type="BitType"/>
    <xs:complexType name="BitType">
        <xs:complexContent>
            <xs:extension base="PrimitiveDataBaseType"> <!-- this is not exactly right because <Bit> doesn't have a 'value'-->
                <xs:attribute name="bit" type="xs:nonNegativeInteger" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="BitString" type="BitStringType"/>
    <xs:complexType name="BitStringType">
        <xs:complexContent>
            <xs:extension base="PrimitiveDataBaseType">
                <xs:sequence>
                    <xs:element name="NamedBits" minOccurs="0">
                        <xs:complexType>
                            <xs:choice maxOccurs="unbounded">
                                <xs:element ref="Bit"/>
                                <xs:any namespace="##other" processContents="lax" minOccurs="0"
                                        maxOccurs="unbounded"/>
                            </xs:choice>
                            <xs:anyAttribute namespace="##other" processContents="lax"/>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="NamedValues" minOccurs="0">
                        <xs:complexType>
                            <xs:choice maxOccurs="unbounded">
                                <xs:element ref="BitString"/>
                                <xs:any namespace="##other" processContents="lax" minOccurs="0"
                                        maxOccurs="unbounded"/>
                            </xs:choice>
                            <xs:anyAttribute namespace="##other" processContents="lax"/>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Value" type="xs:string" minOccurs="0"/>
                </xs:sequence>
                <xs:attribute name="maximumLength" type="xs:nonNegativeInteger" use="optional"/>
                <xs:attribute name="minimumLength" type="xs:nonNegativeInteger" use="optional"/>
                <xs:attribute name="maximumLengthForWriting" type="xs:nonNegativeInteger" use="optional"/>
                <xs:attribute name="minimumLengthForWriting" type="xs:nonNegativeInteger" use="optional"/>
                <xs:attribute name="value" type="xs:string" use="optional"/>
                <xs:attribute name="length" type="xs:nonNegativeInteger" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Boolean" type="BooleanType"/>
    <xs:complexType name="BooleanType">
        <xs:complexContent>
            <xs:extension base="PrimitiveDataBaseType">
                <xs:sequence>
                    <xs:element name="NamedValues" minOccurs="0">
                        <xs:complexType>
                            <xs:choice maxOccurs="unbounded">
                                <xs:element ref="Boolean"/>
                                <xs:any namespace="##other" processContents="lax" minOccurs="0"
                                        maxOccurs="unbounded"/>
                            </xs:choice>
                            <xs:anyAttribute namespace="##other" processContents="lax"/>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="value" type="xs:boolean" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Choice" type="ChoiceType"/>
    <xs:complexType name="ChoiceType">
        <xs:complexContent>
            <xs:extension base="DataBaseType">
                <xs:sequence>
                    <xs:element name="Choices" minOccurs="0">
                        <xs:complexType>
                            <xs:choice minOccurs="0" maxOccurs="unbounded">
                                <xs:group ref="AllDataGroup"/>
                                <xs:any namespace="##other" processContents="lax" minOccurs="0"
                                        maxOccurs="unbounded"/>
                            </xs:choice>
                            <xs:anyAttribute namespace="##other" processContents="lax"/>
                        </xs:complexType>
                    </xs:element>
                    <xs:choice minOccurs="0" >
                        <xs:group ref="AllDataGroup"/>
                    </xs:choice>
                </xs:sequence>
                <xs:attribute name="allowedChoices" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Date" type="DateType"/>
    <xs:complexType name="DateType">
        <xs:complexContent>
            <xs:extension base="PrimitiveDataBaseType">
                <xs:sequence>
                    <xs:element name="NamedValues" minOccurs="0">
                        <xs:complexType>
                            <xs:choice maxOccurs="unbounded">
                                <xs:element ref="Date"/>
                                <xs:any namespace="##other" processContents="lax" minOccurs="0"
                                        maxOccurs="unbounded"/>
                            </xs:choice>
                            <xs:anyAttribute namespace="##other" processContents="lax"/>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="maximum" type="xs:date" use="optional"/>
                <xs:attribute name="minimum" type="xs:date" use="optional"/>
                <xs:attribute name="maximumForWriting" type="xs:date" use="optional"/>
                <xs:attribute name="minimumForWriting" type="xs:date" use="optional"/>
                <xs:attribute name="value" type="xs:date" use="optional"/>
                <xs:attribute name="unspecifiedValue" type="xs:boolean" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="DatePattern" type="DatePatternType"/>
    <xs:complexType name="DatePatternType">
        <xs:complexContent>
            <xs:extension base="PrimitiveDataBaseType">
                <xs:sequence>
                    <xs:element name="NamedValues" minOccurs="0">
                        <xs:complexType>
                            <xs:choice maxOccurs="unbounded">
                                <xs:element ref="DatePattern"/>
                                <xs:any namespace="##other" processContents="lax" minOccurs="0"
                                        maxOccurs="unbounded"/>
                            </xs:choice>
                            <xs:anyAttribute namespace="##other" processContents="lax"/>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="value" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="DateTimePattern" type="DateTimePatternType"/>
    <xs:complexType name="DateTimePatternType">
        <xs:complexContent>
            <xs:extension base="PrimitiveDataBaseType">
                <xs:sequence>
                    <xs:element name="NamedValues" minOccurs="0">
                        <xs:complexType>
                            <xs:choice maxOccurs="unbounded">
                                <xs:element ref="DateTimePattern"/>
                                <xs:any namespace="##other" processContents="lax" minOccurs="0"
                                        maxOccurs="unbounded"/>
                            </xs:choice>
                            <xs:anyAttribute namespace="##other" processContents="lax"/>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="value" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="DateTime" type="DateTimeType"/>
    <xs:complexType name="DateTimeType">
        <xs:complexContent>
            <xs:extension base="PrimitiveDataBaseType">
                <xs:sequence>
                    <xs:element name="NamedValues" minOccurs="0">
                        <xs:complexType>
                            <xs:choice maxOccurs="unbounded">
                                <xs:element ref="DateTime"/>
                                <xs:any namespace="##other" processContents="lax" minOccurs="0"
                                        maxOccurs="unbounded"/>
                            </xs:choice>
                            <xs:anyAttribute namespace="##other" processContents="lax"/>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="maximum" type="xs:dateTime" use="optional"/>
                <xs:attribute name="minimum" type="xs:dateTime" use="optional"/>
                <xs:attribute name="maximumForWriting" type="xs:dateTime" use="optional"/>
                <xs:attribute name="minimumForWriting" type="xs:dateTime" use="optional"/>
                <xs:attribute name="value" type="xs:dateTime" use="optional"/>
                <xs:attribute name="unspecifiedValue" type="xs:boolean" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Double" type="DoubleType"/>
    <xs:complexType name="DoubleType">
        <xs:complexContent>
            <xs:extension base="PrimitiveDataBaseType">
                <xs:sequence>
                    <xs:element name="NamedValues" minOccurs="0">
                        <xs:complexType>
                            <xs:choice maxOccurs="unbounded">
                                <xs:element ref="Double"/>
                                <xs:any namespace="##other" processContents="lax" minOccurs="0"
                                        maxOccurs="unbounded"/>
                            </xs:choice>
                            <xs:anyAttribute namespace="##other" processContents="lax"/>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Units" type="LocalizableStringType" minOccurs="0" maxOccurs="unbounded" />
                </xs:sequence>
                <xs:attribute name="units" type="xs:string" use="optional"/>
                <xs:attribute name="maximum" type="xs:double" use="optional"/>
                <xs:attribute name="minimum" type="xs:double" use="optional"/>
                <xs:attribute name="maximumForWriting" type="xs:double" use="optional"/>
                <xs:attribute name="minimumForWriting" type="xs:double" use="optional"/>
                <xs:attribute name="resolution" type="xs:double" use="optional"/>
                <xs:attribute name="value" type="xs:double" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Enumerated" type="EnumeratedType"/>
    <xs:complexType name="EnumeratedType">
        <xs:complexContent>
            <xs:extension base="PrimitiveDataBaseType">
                <xs:sequence>
                    <xs:element name="NamedValues" minOccurs="0">
                        <xs:complexType>
                            <xs:choice maxOccurs="unbounded">
                                <xs:element ref="Unsigned"/>
                                <xs:any namespace="##other" processContents="lax" minOccurs="0"
                                        maxOccurs="unbounded"/>
                            </xs:choice>
                            <xs:anyAttribute namespace="##other" processContents="lax"/>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="maximum" type="xs:nonNegativeInteger" use="optional"/>
                <xs:attribute name="minimum" type="xs:nonNegativeInteger" use="optional"/>
                <xs:attribute name="maximumForWriting" type="xs:nonNegativeInteger" use="optional"/>
                <xs:attribute name="minimumForWriting" type="xs:nonNegativeInteger" use="optional"/>
                <xs:attribute name="value" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Integer" type="IntegerType"/>
    <xs:complexType name="IntegerType">
        <xs:complexContent>
            <xs:extension base="PrimitiveDataBaseType">
                <xs:sequence>
                    <xs:element name="NamedValues" minOccurs="0">
                        <xs:complexType>
                            <xs:choice maxOccurs="unbounded">
                                <xs:element ref="Integer"/>
                                <xs:any namespace="##other" processContents="lax" minOccurs="0"
                                        maxOccurs="unbounded"/>
                            </xs:choice>
                            <xs:anyAttribute namespace="##other" processContents="lax"/>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Units" type="LocalizableStringType" minOccurs="0" maxOccurs="unbounded" />
                </xs:sequence>
                <xs:attribute name="units" type="xs:string" use="optional"/>
                <xs:attribute name="maximum" type="xs:integer" use="optional"/>
                <xs:attribute name="minimum" type="xs:integer" use="optional"/>
                <xs:attribute name="maximumForWriting" type="xs:integer" use="optional"/>
                <xs:attribute name="minimumForWriting" type="xs:integer" use="optional"/>
                <xs:attribute name="resolution" type="xs:integer" use="optional"/>
                <xs:attribute name="value" type="xs:integer" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Link" type="LinkType"/>
    <xs:complexType name="LinkType">
        <xs:complexContent>
            <xs:extension base="StringType">
                <xs:attribute name="mediaType" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:element name="List" type="ListType"/>
    <xs:complexType name="ListType">
        <xs:complexContent>
            <xs:extension base="CollectionsDataBaseType"/>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Tag" type="TagType"/>
    <xs:complexType name="TagType">
        <xs:complexContent>
            <xs:extension base="DataBaseType">
                <xs:attribute name="equivalentTo" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Null" type="NullType"/>
    <xs:complexType name="NullType">
        <xs:complexContent>
            <xs:extension base="DataBaseType"/>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Object" type="ObjectType"/>
    <xs:complexType name="ObjectType">
        <xs:complexContent>
            <xs:extension base="ConstructedDataBaseType"/>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Composition" type="CompositionType"/>
    <xs:complexType name="CompositionType">
        <xs:complexContent>
            <xs:extension base="ConstructedDataBaseType"/>
        </xs:complexContent>
    </xs:complexType>


    <xs:element name="ObjectIdentifier" type="ObjectIdentifierType"/>
    <xs:complexType name="ObjectIdentifierType">
        <xs:complexContent>
            <xs:extension base="PrimitiveDataBaseType">
                <xs:sequence>
                    <xs:element name="NamedValues" minOccurs="0">
                        <xs:complexType>
                            <xs:choice maxOccurs="unbounded">
                                <xs:element ref="ObjectIdentifier"/>
                                <xs:any namespace="##other" processContents="lax" minOccurs="0"
                                        maxOccurs="unbounded"/>
                            </xs:choice>
                            <xs:anyAttribute namespace="##other" processContents="lax"/>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="maximum" type="xs:nonNegativeInteger" use="optional"/>
                <xs:attribute name="minimum" type="xs:nonNegativeInteger" use="optional"/>
                <xs:attribute name="maximumForWriting" type="xs:nonNegativeInteger" use="optional"/>
                <xs:attribute name="minimumForWriting" type="xs:nonNegativeInteger" use="optional"/>
                <xs:attribute name="value" type="xs:string" use="optional"/>
                <xs:attribute name="unspecifiedValue" type="xs:boolean" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="ObjectIdentifierPattern" type="ObjectIdentifierPatternType"/>
    <xs:complexType name="ObjectIdentifierPatternType">
        <xs:complexContent>
            <xs:extension base="PrimitiveDataBaseType">
                <xs:sequence>
                    <xs:element name="NamedValues" minOccurs="0">
                        <xs:complexType>
                            <xs:choice maxOccurs="unbounded">
                                <xs:element ref="ObjectIdentifierPattern"/>
                                <xs:any namespace="##other" processContents="lax" minOccurs="0"
                                        maxOccurs="unbounded"/>
                            </xs:choice>
                            <xs:anyAttribute namespace="##other" processContents="lax"/>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="value" type="xs:string" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="OctetString" type="OctetStringType"/>
    <xs:complexType name="OctetStringType">
        <xs:complexContent>
            <xs:extension base="PrimitiveDataBaseType">
                <xs:sequence>
